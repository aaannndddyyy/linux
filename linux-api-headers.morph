{
    "name": "linux-api-headers",
    "kind": "chunk",
    "install-commands": [
        "make mrproper",
        "ARCH=$(./morph-arch) make INSTALL_HDR_PATH=dest headers_install",
        "install -d \"$DESTDIR${PREFIX-/usr}/include\"",
        "cp -r dest/include/* \"$DESTDIR/${PREFIX-/usr}/include\""
    ]
}
