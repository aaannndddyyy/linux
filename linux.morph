name: linux
kind: chunk
configure-commands:
    - cp arch/arm/configs/wandboard_defconfig config
    - scripts/config -e NAMESPACES
    - yes '' | make oldconfig
build-commands:
    - make $MAKEFLAGS ARCH=arm uImage
install-commands:
    - mkdir -p "$DESTDIR"/boot
    - cp arch/arm/boot/uImage "$DESTDIR"/boot/uImage

